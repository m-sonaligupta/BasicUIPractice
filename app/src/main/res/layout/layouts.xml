<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#00BCD4">

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Welcome to the Complete Android Q App Development  Course!"
        android:textColor="#AA0A0A"
        app:layout_constraintBottom_toTopOf="@+id/relativeLayout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.16" />

    <Button
        android:id="@+id/button3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="48dp"
        android:text="LETS GET STARTED!"
        android:textColor="#A11919"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <RelativeLayout
        android:id="@+id/relativeLayout"
        android:layout_width="408dp"
        android:layout_height="92dp"
        tools:layout_editor_absoluteX="-1dp"
        tools:layout_editor_absoluteY="125dp">

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="89dp"
            android:layout_marginLeft="89dp"
            android:layout_marginEnd="76dp"
            android:layout_marginRight="76dp"
            android:layout_marginBottom="67dp"
            android:text="Android App Development is Awsome!"
            android:textColor="#6A1010" />

        <Button
            android:id="@+id/button7"
            android:layout_width="186dp"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="113dp"
            android:layout_marginLeft="113dp"
            android:layout_marginEnd="109dp"
            android:layout_marginRight="109dp"
            android:layout_marginBottom="9dp"
            android:text="WHAT DO YOU THINK?"
            android:textColor="#781818" />
    </RelativeLayout>

    <LinearLayout
        android:layout_width="414dp"
        android:layout_height="123dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="234dp">

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_margin="10dp"
            android:text="Java Programming is Fantastic!"
            android:textColor="#6C1A1A" />

        <Button
            android:id="@+id/button8"
            android:layout_width="180dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_margin="20dp"
            android:layout_marginTop="10dp"
            android:gravity="center"
            android:text="LEARN JAVA"
            android:textColor="#871F1F" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="406dp"
        android:layout_height="78dp"
        android:orientation="horizontal"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="381dp">

        <Button
            android:id="@+id/button11"
            android:layout_width="40dp"
            android:layout_height="wrap_content"
            android:layout_weight="5"
            android:text="1" />

        <Button
            android:id="@+id/button12"
            android:layout_width="40dp"
            android:layout_height="wrap_content"
            android:layout_weight="5"
            android:text="2" />

        <Button
            android:id="@+id/button13"
            android:layout_width="40dp"
            android:layout_height="wrap_content"
            android:layout_weight="5"
            android:text="3" />

        <Button
            android:id="@+id/button16"
            android:layout_width="40dp"
            android:layout_height="wrap_content"
            android:layout_weight="5"
            android:text="4" />

        <Button
            android:id="@+id/button18"
            android:layout_width="40dp"
            android:layout_height="wrap_content"
            android:layout_weight="5"
            android:text="5" />

        <Button
            android:id="@+id/button14"
            android:layout_width="40dp"
            android:layout_height="wrap_content"
            android:layout_weight="5"
            android:text="6" />

        <Button
            android:id="@+id/button15"
            android:layout_width="40dp"
            android:layout_height="wrap_content"
            android:layout_weight="5"
            android:text="7" />

        <Button
            android:id="@+id/button17"
            android:layout_width="40dp"
            android:layout_height="wrap_content"
            android:layout_weight="5"
            android:text="8" />

        <Button
            android:id="@+id/button19"
            android:layout_width="40dp"
            android:layout_height="wrap_content"
            android:layout_weight="5"
            android:text="9" />
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>